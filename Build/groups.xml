<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Master Suite">
	
		
	<groups>
		<run>
			<include name="Master"></include>
			<!--<include name="Regression"></include>-->
			<exclude name="Datadriven"></exclude>
		</run>
	</groups>
	<listeners>
		<listener class-name="com.OpenCart.Utility.ExtentReportManager"></listener>
	</listeners>
	
	
  <test thread-count="5" name="Test">

  <parameter name="os" value="mac" />
  <parameter name="browser" value="firefox" />

    <classes>
		
	<class name="com.OpenCart.HomePage.TC_HomePage"/>
	 <class name="com.OpenCart.Register.TC_RegisterPage"/>
		<class name="com.OpenCart.Login.TC_LoginPage"/>
		<class name="com.OpenCart.Login.TC_LoginPageDDT"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
